package com.myworkings;

import java.util.ArrayList;
import java.util.Collection;
import java.util.Collections;
import java.util.LinkedHashSet;
import java.util.List;

public class ListRmDuplicates {
	public static void main(String[] args) {
		ArrayList<String> arrayList = new ArrayList<String>();
		arrayList.add("Sudharsan");
		arrayList.add("Ravi");
		arrayList.add("Ravi");
		System.out.println("Duplicates List");
		LinkedHashSet<String> a = new LinkedHashSet<>(arrayList);
		System.out.println("Without Duplicates List");

		for (String string : a) {
			System.out.println(string);
		}
		Collections.sort(a,Collections.reverseOrder());
	}

}
